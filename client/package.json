{
  "name": "front-end",
  "version": "0.0.1",
  "scripts": {
    "watch:sass": "node-sass styles/sass/main.scss styles/css/style.css -w",
    "dev": "vite",
    "start": "npm-run-all --parallel dev watch:sass",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "compile:sass": "node-sass styles/sass/main.scss styles/css/style.comp.css",
    "concat:css": "concat -o styles/css/style.concat.css styles/css/style.comp.css",
    "prefix:css": "postcss --user autoprefixer -b 'last 10 versions' styles/css/style.concat.css -o styles/css/style.prefix.css",
    "compress:css": "node-sass styles/css/style.prefix.css styles/css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css"
  },
  "dependencies": {
    "@types/react-router-dom": "^5.3.3",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-router-dom": "^6.3.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@vitejs/plugin-react": "^1.0.0",
    "autoprefixer": "^10.4.8",
    "concat": "^1.0.3",
    "node-sass": "^7.0.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^10.0.0",
    "typescript": "^4.3.2",
    "vite": "^2.5.4"
  }
}
